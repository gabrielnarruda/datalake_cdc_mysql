create table tb_dim_olist_order_items  (
    id int primary key auto_increment,
    order_id varchar(255),
    order_item_id int,
    product_id varchar(255),
    seller_id varchar(255),
    price numeric,
    freight_value numeric,
    created_at datetime,
    updated_at datetime,
    created_by varchar(255),
    updated_by varchar(255))